<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.max.back.neusoft.dao.ScheduleMapper">

    <resultMap id="BaseResultMap" type="com.max.back.neusoft.pojo.Schedule">
            <id property="id" column="s_id" jdbcType="VARCHAR"/>
            <result property="name" column="s_name" jdbcType="VARCHAR"/>
            <result property="start" column="s_start" jdbcType="TIME"/>
            <result property="end" column="s_end" jdbcType="TIME"/>
    </resultMap>

    <sql id="Base_Column_List">
        s_id,s_name,s_start,
        s_end
    </sql>
</mapper>
